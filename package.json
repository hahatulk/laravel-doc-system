{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "dep": "composer update && composer install && npm install",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "docker": "docker-compose up --build -d",
        "cache": "docker-compose run workspace php artisan config:cache && docker-compose run workspace php artisan config:clear",
        "migrate": "docker-compose run workspace php artisan migrate:fresh --seed",
        "larkey": "docker-compose run workspace composer run-script post-create-project-cmd"
    },
    "devDependencies": {
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
